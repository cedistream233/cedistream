{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "web-quick-build",
			"type": "shell",
			"command": "npm -ws run -s build",
			"problemMatcher": [
				"$tsc",
				"$eslint - stylish"
			],
			"group": "build"
		},
		{
			"label": "backend-start-check",
			"type": "shell",
			"command": "node -e \"console.log('node ok')\"",
			"group": "build"
		},
		{
			"label": "start-frontend-dev",
			"type": "shell",
			"command": "npm --workspace frontend run dev",
			"isBackground": true,
			"group": "build"
		},
		{
			"label": "frontend-build-check",
			"type": "shell",
			"command": "npm --workspace frontend run build",
			"group": "build"
		},
		{
			"label": "backend-lint-check",
			"type": "shell",
			"command": "node -e \"console.log('ok')\"",
			"group": "build"
		},
		{
			"label": "frontend-build-check",
			"type": "shell",
			"command": "npm --workspace frontend run build",
			"group": "build"
		},
		{
			"label": "frontend-build-check",
			"type": "shell",
			"command": "npm --workspace frontend run build",
			"group": "build"
		},
		{
			"label": "frontend-build-check",
			"type": "shell",
			"command": "npm --workspace frontend run build",
			"group": "build"
		}
	]
}